<div class="trunkstore_popup e911">
    <div class="row-fluid">
        <div class="span12">
            <span class="e911_text">{{t "pbxs.pbxs_manager.when_you_dial"}}<span class="big_blue_text">{{t "pbxs.pbxs_manager.emergency_number"}}</span>{{t "pbxs.pbxs_manager.from"}}<br/>{{t "pbxs.pbxs_manager.end_when_you_dial"}}<i class="icon-question-sign" data-toggle="tooltip" data-placement="left" data-original-title="{{t "pbxs.pbxs_manager.help_e911"}}"></i></span>
        </div>
    </div>

    <div class="row-fluid">
        <div class="span9">
            <form id="e911" class="form-horizontal">
                <div class="control-group">
                    <label class="control-label" for="postal_code">{{t "pbxs.pbxs_manager.postal_code"}}</label>
                    <div class="controls">
                        <input id="postal_code" type="text" name="postal_code" value="{{postal_code}}"/>
                    </div>
                </div>
                <div class="control-group">
                    <label class="control-label" for="street_address">{{t "pbxs.pbxs_manager.address_line_1"}}</label>
                    <div class="controls">
                        <input type="text" name="street_address" value="{{street_address}}" />
                    </div>
                </div>
                <div class="control-group">
                    <label class="control-label" for="extended_address">{{t "pbxs.pbxs_manager.address_line_2"}}</label>
                    <div class="controls">
                        <input type="text" name="extended_address" value="{{extended_address}}" />
                    </div>
                </div>
                <div class="control-group">
                    <label class="control-label" for="locality">{{t "pbxs.pbxs_manager.city"}}</label>
                    <div class="controls">
                        <input id="locality" type="text" name="locality" value="{{locality}}" />
                    </div>
                </div>
                <div class="control-group">
                    <label class="control-label" for="region">{{t "pbxs.pbxs_manager.state"}}</label>
                    <div class="controls">
                        <input id="region" type="text" name="region" value="{{region}}" />
                    </div>
                </div>
                {{#if status}}
                    <div class="control-group gmap_link_div" style="display: none;">
                        <div class="controls">
                            <a class="monster_link" id="gmap_link" href="http://maps.google.com/maps?q=${latitude},+${longitude}+(Your+E911+Location)&iwloc=A&hl=en" target="_blank">{{t "pbxs.pbxs_manager.verify_location"}}</a>
                        </div>
                    </div>
                {{/if}}
            </form>
        </div>

        <div class="span3">
            <div class="e911_image"/>
            <button class="btn btn-success submit_btn">{{t "pbxs.pbxs_manager.submit"}}</button>
        </div>
    </div>
</div>
