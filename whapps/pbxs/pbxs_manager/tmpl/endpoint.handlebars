                                                            </tr>
                                                            <tr>
                                                                <td><i class="icon-ok-sign icon-green"></i></td>
                                                                <td>{{t "pbxs.pbxs_manager.pbxs_responds"}}</td>
                                                            </tr>
                                                        </table>
