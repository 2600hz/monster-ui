                                            {{t "pbxs.pbxs_manager.tell_us_a_number"}}
                                        </div>
                                        <div class="row-fluid content-centered">
                                            <input type="text" id="test_number" placeholder="555-555-5555"></input>
                                        </div>
                                        <div class="row-fluid">
                                            <div class="help-box gray-box">
                                                <div class="wrapper-icon">
                                                    &nbsp;<i class="icon-info-sign2"></i>&nbsp;
                                                </div>
                                                <div class="text-wrapper">
                                                    <p>{{t "pbxs.pbxs_manager.help_simulate_call"}}</p>
                                                </div>
                                            </div>
                                        </div>

                                        <div class="row-fluid">
                                            <button id="start_test" class="btn btn-large btn-success pull-right">{{t "pbxs.pbxs_manager.start_test"}}</button>
                                            <button id="cancel_test" class="btn btn-large btn-danger">{{t "pbxs.pbxs_manager.cancel_test"}}</button>
                                        </div>
                                    </div>

                                    <div class="wizard-automatic-step" data-value="2">
                                        <div class="well">
                                            <div class="wizard-automatic-step-title">
                                                <i class="icon-rss"></i>{{t "pbxs.pbxs_manager.firewall_settings"}}
                                                <span class="pull-right"><a href="#" class="monster_link skip_test">[ {{t "pbxs.pbxs_manager.skip_test"}} ]</a></span>
                                            </div>

                                            <div class="wizard-automatic-step-content">
                                                <div class="row-fluid">
                                                    <div class="span6">
                                                        <table>
                                                            <tr>
                                                                <td><i class="icon-ok-sign icon-green"></i></td>
                                                                <td>{{t "pbxs.pbxs_manager.valid_registration"}}</td>
                                                            </tr>
                                                            <tr>
                                                                <td><i class="icon-ok-sign icon-green"></i></td>
                                                                <td>{{t "pbxs.pbxs_manager.respond_to"}}</td>
                                                            </tr>
                                                            <tr>
                                                                <td><i class="icon-ok-sign icon-green"></i></td>
                                                                <td>{{t "pbxs.pbxs_manager.continues_responding"}}</td>
                                                            </tr>
                                                        </table>
                                                    </div>
                                                    <div class="span6">
                                                        <div class="row-fluid">
                                                            <div class="help-box gray-box">
                                                                <div class="wrapper-icon">
                                                                    &nbsp;<i class="icon-info-sign2"></i>&nbsp;
                                                                </div>
                                                                <div class="text-wrapper">
                                                                    <p>{{t "pbxs.pbxs_manager.help_automatic_step_2"}}</p>
                                                                </div>
                                                            </div>
                                                        </div>

                                                        <div class="row-fluid">
                                                            <div class="progress-title">{{t "pbxs.pbxs_manager.current_testing"}}</div>
                                                            <div class="progress progress-success progress-striped active">
                                                                <div class="bar"></div>
                                                            </div>
                                                        </div>
                                                    </div>
                                                </div>
                                            </div>
                                        </div>
                                    </div>

                                    <div class="wizard-automatic-step" data-value="3">
