{{#each unassigned_numbers}}
    <div class="unassigned-number number-box" data-phone_number="{{this}}">
        <input type="checkbox"></input>
        <span class="phone-number">{{format_phone_number this}}</span>
    </div>
{{else}}
    <div class="unassigned-number no-number">{{t "pbxs.pbxs_manager.no_number_available"}}</div>
{{/each}}
