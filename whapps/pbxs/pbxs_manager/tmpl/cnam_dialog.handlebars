<div class="trunkstore_popup cnam clearfix">
    <div class="row-fluid">
        <div class="left_image"></div>

        <div class="right_part">
            <div class="row-fluid">
                <div class="row-fluid">
                    Caller ID Name
                </div>

                <div class="row-fluid">
                    <form id="cnam">
                        <input type="text" class="caller_id" name="display_name" value="{{display_name}}" placeholder="John Doe"/>
                    </form>
                </div>

                <div class="row-fluid">
                    <button class="btn btn-success">Add</button>
                </div>
            </div>
        </div>
    </div>

    <div class="row-fluid">
        <div class="help-box gray-box">
            <div class="icon-wrapper">
                <i class="icon-info-sign2"></i>
            </div>
            <div class="text-wrapper">
                <p>This name will show up when you call someone on their Caller ID device.</p>
                <p>This request takes up to 72 hours to process and can only be placed once every 72 hours.</p>
            </div>
        </div>
    </div>
</div>
